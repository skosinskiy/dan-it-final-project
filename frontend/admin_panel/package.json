{
  "name": "admin_panel",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.1",
    "react-redux-toastr": "^7.4.8",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.5",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "3.3.0"
  },
  "devDependencies": {
    "@types/node": "^11.11.4",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.1",
    "npm-watch": "^0.3.0",
    "redux-devtools-extension": "^2.13.8",
    "standard": "^10.0.3",
    "stylelint": "^8.4.0",
    "stylelint-config-rational-order": "0.0.2",
    "stylelint-order": "^2.0.0",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "prettier": "^1.16.4"
  },
  "watch": {
    "stylelint-dev": {
      "patterns": [
        "/src/**/*.scss"
      ],
      "extensions": "scss"
    }
  },
  "scripts": {
    "start": "npm-run-all -p watch-styles-sass start-js",
    "start-js": "react-scripts start",
    "build": "npm run eslint-build && npm run stylelint-build && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "stylelint-dev": "stylelint \"src/**/*.*css\" --cache --cache-location \"./.stylelintcache\"; exit 0",
    "stylelint-build": "stylelint \"src/**/*.*css\"",
    "watch-styles-sass": "npm-watch",
    "stylelint-fix-sass": "stylelint --fix \"src/**/*.scss\"",
    "eslint-build": "eslint src/**/*.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:9000"
}
