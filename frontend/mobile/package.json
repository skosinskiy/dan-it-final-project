{
  "name": "mobile",
  "version": "0.1.0",
  "homepage": "/mobile",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "prop-types": "^15.5.7",
    "react": "^16.8.6",
    "react-autosize-textarea": "^7.0.0",
    "react-dom": "^16.8.3",
    "react-iframe": "^1.8.0",
    "react-preloaders": "^2.1.4",
    "react-redux": "^6.0.0",
    "react-redux-toastr": "^7.4.8",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.5",
    "react-scroll-to-bottom": "^1.3.1",
    "react-select": "^2.4.3",
    "react-stomp": "^4.1.1",
    "react-swipe": "^6.0.4",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "rss-parser": "^3.7.0",
    "swipe-js-iso": "^2.1.5",
    "ts-pnp": "^1.0.1",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.1",
    "npm-watch": "^0.3.0",
    "redux-devtools-extension": "^2.13.8",
    "standard": "^10.0.3",
    "stylelint": "^9.8.0",
    "stylelint-config-rational-order": "0.0.2",
    "stylelint-order": "^2.0.0"
  },
  "watch": {
    "stylelint-dev": {
      "patterns": [
        "/src/**/*.scss"
      ],
      "extensions": "scss"
    }
  },
  "scripts": {
    "start": "npm-run-all -p watch-styles-sass start-js",
    "start-js": "react-scripts start",
    "build": "npm run eslint-build && npm run stylelint-build && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "stylelint-dev": "stylelint \"src/**/*.*css\" --cache --cache-location \"./.stylelintcache\"; exit 0",
    "stylelint-build": "stylelint \"src/**/*.*css\"",
    "watch-styles-sass": "npm-watch",
    "stylelint-fix-sass": "stylelint --fix \"src/**/*.scss\"",
    "eslint-build": "eslint \"src/**/*.js\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:9000"
}
